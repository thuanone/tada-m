  "private": true,
  "main": "dist/server/ts/app.js",
  "scripts": {
    "analyze": "ANALYZE_BUNDLE_SIZE=true npm run deploy",
    "build": "run-s compile deploy",
    "build:dev": "run-p compile:dev deploy:dev",
    "check": "npm run lint && npm run test",
    "clean": "npm cache clean --force && rm -rf node_modules package-lock.json && npm i",
    "compile": "node prepareBuild.js && tsc",
    "compile:dev": "node prepareBuild.js && tsc-watch --noClear",
    "deploy": "NODE_ENV=production node_modules/.bin/gulp deploy --env=prod",
    "deploy:dev": "NODE_ENV=development node_modules/.bin/gulp deploy --env=dev",
    "dev": "npm run proxy:start && run-p build:dev start:dev",
    "dev:light": "run-p build:dev start:dev",
    "generate:key": "node dev/session-key-generator.js",
    "i18n": "node_modules/.bin/gulp i18n",
    "lint": "run-p lint:es lint:sass lint:dust lint:json lint:ts lint:sh",
    "lint:dust": "node_modules/.bin/dustmite --path src/server/views",
    "lint:es": "node_modules/.bin/eslint --ext .js,.jsx",
    "lint:json": "node_modules/.bin/jsonlint --quiet *.json config/*.json",
    "lint:sass": "./node_modules/sass-lint/bin/sass-lint.js -c ./.sass-lint.yml -q -v",
    "lint:sh": "bash -n *.sh",
    "lint:ts": "tslint -c tslint.json -p tsconfig_lint.json",
    "opensource": "node_modules/.bin/gulp opensource",
    "prebuild": "npm run lint:ts",
    "proxy:start": "dev-proxy configure whisk --location dev/whisk.loc --skip --dockernetwork off --debug",
    "proxy:stop": "dev-proxy stop",
    "watch": "node_modules/.bin/gulp serve",
    "watch:dev": "node_modules/.bin/gulp serve_only",
    "start": "node dist/server/ts/app.js",
    "start:dev": "run-p node:dev watch:dev",
    "start:prod": "npm run build && NODE_ENV='production' port=39003 bash ./dev/start-server.sh",
    "node:dev": "NODE_ENV='development' bash ./dev/start-server.sh",
    "nightwatch": "bash ./src/e2e/run.sh",
    "nightwatch:basic": "E2E_BASIC_TEST=true bash ./src/e2e/run.sh",
    "nightwatch:devtest": "bash ./src/e2e/run.sh devtest",
    "nightwatch:devprod": "bash ./src/e2e/run.sh devprod",
    "nightwatch:ondecktest": "bash ./src/e2e/run.sh ondecktest",
    "nightwatch:ondeckprod": "bash ./src/e2e/run.sh ondeckprod",
    "nightwatch:test": "bash ./src/e2e/run.sh test",
    "nightwatch:prod": "bash ./src/e2e/run.sh prod",
    "test": "run-s test_server test_client test_coverage",
    "test_client": "jest",
    "test_coverage": "istanbul report --config './.istanbul-coverage.yml' --include './coverage/**/coverage-*.json' && cat './coverage/text_summary.txt' && mkdir -p ./reports/coverage && mv ./coverage/*.xml ./reports/coverage",
    "test:client": "npm run test_client && npm run test_coverage",
    "test:server": "npm run test_server && npm run test_coverage",
    "test_server:no-coverage": "ts-node node_modules/jasmine/bin/jasmine.js JASMINE_CONFIG_PATH=jasmine.json",
    "test_server": "ts-node node_modules/istanbul/lib/cli.js cover -e .ts  -x \"*.d.ts\" -x \"*.spec.ts\" -x \"src/server/__tests__/**/*\" -x \"src/client/**/*\" node_modules/jasmine/bin/jasmine.js --print detail -- JASMINE_CONFIG_PATH=jasmine.json",
    "test:unit": "npm run test",
    "test:e2e-x": "E2E_BASIC_TEST=false ./node_modules/.bin/nightwatch --suiteRetries 0"
  },
  "engines": {
    "node": "^12.x",
    "npm": "6.*"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
